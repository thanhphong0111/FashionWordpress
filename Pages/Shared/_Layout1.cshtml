    <script>
        // Select all customer và Block cus select item
        document.addEventListener('DOMContentLoaded', function () {
            var selectAllCheckbox = document.getElementById('selectAllCustomer');
            var checkboxes = document.querySelectorAll('input[name="selectedProductIds"]');
            var btnDelete = document.getElementById('btnBlock');

            selectAllCheckbox.addEventListener('change', function () {
                var isChecked = this.checked;
                checkboxes.forEach(function (checkbox) {
                    checkbox.checked = isChecked;
                });

                toggleDeleteButton();
            });

            checkboxes.forEach(function (checkbox) {
                checkbox.addEventListener('change', function () {
                    toggleDeleteButton();
                });
            });

            function toggleDeleteButton() {
                var anyChecked = false;
                checkboxes.forEach(function (checkbox) {
                    if (checkbox.checked) {
                        anyChecked = true;
                        return;
                    }
                });

                if (anyChecked) {
                    btnBlock.disabled = false;
                } else {
                    btnBlock.disabled = true;
                }
            }
        });

        document.addEventListener('DOMContentLoaded', function () {
            var selectAllCheckbox = document.getElementById('selectAllEmployee');
            var checkboxes = document.querySelectorAll('input[name="selectedEmployeeId"]');
            var btnDelete = document.getElementById('btnBlock');

            selectAllCheckbox.addEventListener('change', function () {
                var isChecked = this.checked;
                checkboxes.forEach(function (checkbox) {
                    checkbox.checked = isChecked;
                });

                toggleDeleteButton();
            });

            checkboxes.forEach(function (checkbox) {
                checkbox.addEventListener('change', function () {
                    toggleDeleteButton();
                });
            });

            function toggleDeleteButton() {
                var anyChecked = false;
                checkboxes.forEach(function (checkbox) {
                    if (checkbox.checked) {
                        anyChecked = true;
                        return;
                    }
                });

                if (anyChecked) {
                    btnBlock.disabled = false;
                } else {
                    btnBlock.disabled = true;
                }
            }
        });

            document.addEventListener('DOMContentLoaded', function () {
                var selectAllCheckbox = document.getElementById('selectAllCategory');
                var checkboxes = document.querySelectorAll('input[name="selectedCategoryId"]');
                var btnDelete = document.getElementById('btnDelete');




                selectAllCheckbox.addEventListener('change', function () {
                    var isChecked = this.checked;
                    checkboxes.forEach(function (checkbox) {
                        checkbox.checked = isChecked;
                    });

                    toggleDeleteButton();
                });

                checkboxes.forEach(function (checkbox) {
                    checkbox.addEventListener('change', function () {
                        toggleDeleteButton();
                    });
                });

                function toggleDeleteButton() {
                    var anyChecked = false;
                    checkboxes.forEach(function (checkbox) {
                        if (checkbox.checked) {
                            anyChecked = true;
                            return;
                        }
                    });

                    if (anyChecked) {
                        btnDelete.disabled = false;
                    } else {
                        btnDelete.disabled = true;
                    }
                }
            });


            $(function () {
                $("#example1").DataTable({
                    "responsive": true,
                    "lengthChange": true,
                    "autoWidth": false,
                    "buttons": ["copy", "csv", "excel", "pdf", "print", "colvis"],
                    "pageLength": 25 // Số bản ghi trong một trang
                }).buttons().container().appendTo('#example1_wrapper .col-md-6:eq(0)');
            });

            // Hiển thị Message Box thông báo
            $(function () {
                $('#AlertBox').removeClass('hide');
                $('#AlertBox').delay(3000).slideUp(500);

            });

            // Hiển thị popup edit category details
            window.addEventListener("DOMContentLoaded", function () {
                var myModal = new bootstrap.Modal(document.getElementById("editCategoryModal"));
                myModal.show();
            });


            //Editor blog
        $(function () {
